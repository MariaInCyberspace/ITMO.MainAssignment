<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.LostAndFound_1" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/my_yellow</item>
        <item name="colorPrimaryVariant">@color/my_green</item>
        <item name="colorOnPrimary">@color/my_grey</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/my_green</item>
        <item name="colorSecondaryVariant">@color/my_green</item>
        <item name="colorOnSecondary">@color/my_grey</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="fontFamily">@font/comfortaa</item>
    </style>


    <style name="Theme.LostAndFound_1.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="fontFamily">@font/comfortaa</item>
    </style>


    <style name="Theme.LostAndFound_1.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.LostAndFound_1.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>